{
  "manifest_version": 2,
  "name": "Jagran EPaper Full Access",  
  "version": "1.1",
  
  "description": "E-paper full access. It is a proof of concept job. In case of copyright conflict, please let me know.",

  "browser_action": {
    "default_icon": "jagranIcon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
      "128": "jagranIcon128.png",
	    "64": "jagranIcon64.png",
      "48": "jagranIcon48.png"
   },
  "permissions": [
    "https://epaper.jagran.com/*"
  ],
 "content_scripts": [{
        "matches": ["https://epaper.jagran.com/*"],
        "js": ["content.js"]
    }]
}